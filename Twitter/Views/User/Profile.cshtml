@model Twitter.Models.Register

@{
    ViewData["Title"] = "Profile";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<h1>Profile</h1>
@if (ViewBag.UserData != null)
{
    <div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <label>Name : </label>
        </dt>
        <dd class = "col-sm-10">
          @ViewBag.UserData.name
        </dd>
        <dt class = "col-sm-2">
            <label>Email : </label>
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.UserData.Email
        </dd>
        <dt class = "col-sm-2">
            <label>UserName : </label>
        </dt>
        <dd class = "col-sm-10">
           @ViewBag.UserData.userName
        </dd>
    </dl>
</div>
}


